<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mycard_ios">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/appbar_scrolling_view_behavior"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyCard_ios">
        <activity android:name=".Awardsarea"></activity>
        <activity
            android:name=".Verification"
            android:label="@string/title_activity_verification"
            android:theme="@style/Theme.MyCard_ios.NoActionBar" />
        <activity
            android:name=".Password"
            android:label="@string/title_activity_password"
            android:theme="@style/Theme.MyCard_ios.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>